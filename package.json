{
  "name": "nd-rc",
  "version": "0.0.1",
  "description": "React Components",
  "main": "lib/index",
  "jsnext:main": "es6/index",
  "files": [
    "*.md",
    "demo",
    "es6",
    "lib",
    "umd",
    "src"
  ],
  "keywords": [
    "react",
    "reactjs",
    "react-component"
  ],
  "scripts": {
    "start": "npm run demo",
    "build": "npm run build-cjs",
    "build-cjs": "rimraf lib && babel ./src -d lib",
    "build-es6": "rimraf es6 && babel ./src -d es6 --blacklist=es6.modules",
    "build-umd": "set NODE_ENV=production && webpack src/index.js umd/nd-rc.js",
    "build-min": "set NODE_ENV=production && webpack -p src/index.js umd/nd-rc.min.js",
    "demo": "webpack-dev-server  --progress --port 3000  --colors --content-base demo --hot --inline --config demo/webpack.config.js",
    "test": "karma start test/karma.conf.js",
    "test:dev": "karma start test/karma.conf.js --browsers=Chrome --single-run=false",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "bundle-analyse": "NODE_ENV=production webpack src/index.js umd/nd-rc.js --json | webpack-bundle-size-analyzer"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ndfront/nd-rc.git"
  },
  "author": "ND Front",
  "bugs": {
    "url": "https://github.com/ndfront/nd-rc/issues"
  },
  "homepage": "https://github.com/ndfront/nd-rc.git",
  "peerDependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "dependencies": {
    "core-decorators": "^0.11.0",
    "core-js": "2.1.1",
    "spin.js": "^2.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.2",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-enzyme": "^0.4.0",
    "cheerio": "^0.20.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "enzyme": "^2.0.0",
    "eslint": "^2.2.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^2.2.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-flow-vars": "^0.2.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^4.0.0",
    "eslint-plugin-standard": "^1.3.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack-with-fast-source-maps": "^1.9.2",
    "mocha": "^2.3.4",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-loader": "^0.8.1",
    "pre-commit": "^1.1.2",
    "react": "^0.14.0",
    "react-addons-linked-state-mixin": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-addons-transition-group": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.0.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-bundle-size-analyzer": "^2.0.1",
    "webpack-dev-server": "^1.14.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
